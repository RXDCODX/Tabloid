@using Microsoft.AspNetCore.Hosting
@using Microsoft.Extensions.Hosting

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width"/>
    <title>@ViewBag.Title</title>
    <link href="~/styles/test1.css" rel="stylesheet" type="text/css">
    <link href="~/styles/test2.css" rel="stylesheet" type="text/css">
    <link href="~/styles/test3.css" rel="stylesheet" type="text/css">
    <script src="~/lib/microsoft/signalr/dist/browser/signalr.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="scripts/MainModel.js?v=@DateTime.Now.Ticks"></script>
    @await Html.PartialAsync("_ValidationScriptsPartial")
</head>
<body data-bs-theme="dark">
    @RenderBody()
<script src="/scripts/_Layout.js?v=@DateTime.Now.Ticks"></script>
</body>
</html>
