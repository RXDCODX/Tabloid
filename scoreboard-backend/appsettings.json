{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5073"
      }
    }
  },
  "url": "http://*:5073",
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "spa": {
        "ClusterId": "spaCluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "spaCluster": {
        "Destinations": {
          "spa": {
            "Address": "http://localhost:5173"
          }
        }
      }
    }
  }
}

